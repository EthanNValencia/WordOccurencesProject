<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WordCounter.Controller">
   <children>
      <ImageView fx:id="imageView" fitHeight="254.0" fitWidth="606.0" layoutX="14.0" layoutY="68.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="beginBtn" layoutX="310.0" layoutY="333.0" mnemonicParsing="false" onAction="#downloadPage" text="Download HTML" />
      <TextArea fx:id="textArea" layoutX="14.0" layoutY="36.0" prefHeight="294.0" prefWidth="871.0" />
      <Button fx:id="cleanBtn" layoutX="425.0" layoutY="333.0" mnemonicParsing="false" onAction="#cleanText" text="Clean Text" />
      <Button fx:id="countBtn" layoutX="504.0" layoutY="333.0" mnemonicParsing="false" onAction="#countWords" text="Count Words" />
      <Label layoutX="450.0" layoutY="345.0" />
      <Label fx:id="labelExplain" layoutX="14.0" layoutY="14.0" text="Label" />
      <Button fx:id="connect" layoutX="310.0" layoutY="362.0" mnemonicParsing="false" onAction="#getIP" text="Connect" />
      <Button fx:id="noConnect" layoutX="380.0" layoutY="362.0" mnemonicParsing="false" onAction="#doNotConnect" text="Do Not Connect" />
      <Button fx:id="submitBtn" layoutX="14.0" layoutY="362.0" mnemonicParsing="false" onAction="#beginConnection" text="Submit IP Address" />
      <TextField fx:id="ipEntry" layoutX="14.0" layoutY="333.0" prefHeight="25.0" prefWidth="172.0" promptText="Enter IP Address..." />
      <Button fx:id="restartBtn" layoutX="833.0" layoutY="362.0" mnemonicParsing="false" onAction="#initButtons" text="Restart" />
      <Button fx:id="quitBtn" layoutX="847.0" layoutY="6.0" mnemonicParsing="false" onAction="#quitSystem" text="Quit" />
      <Button fx:id="option1" layoutX="320.0" layoutY="344.0" mnemonicParsing="false" onAction="#helpCenter_Option1" prefHeight="39.0" prefWidth="71.0" text="1" />
      <Button fx:id="option2" layoutX="414.0" layoutY="343.0" mnemonicParsing="false" onAction="#helpCenter_Option2" prefHeight="39.0" prefWidth="71.0" text="2" />
      <Button fx:id="option3" layoutX="504.0" layoutY="343.0" mnemonicParsing="false" onAction="#helpCenter_Option3" prefHeight="39.0" prefWidth="71.0" text="3" />
      <Button fx:id="helpCenterBtn" layoutX="704.0" layoutY="362.0" mnemonicParsing="false" onAction="#returnToHelpCenter" text="Help Center" />
      <Button fx:id="opt2Next" layoutX="577.0" layoutY="362.0" mnemonicParsing="false" onAction="#helpCenter_Option2" text="Next &gt;" />
      <Button fx:id="opt2Back" layoutX="495.0" layoutY="362.0" mnemonicParsing="false" onAction="#helpCenter_Option2" text="&lt; Back" />
   </children>
</AnchorPane>
