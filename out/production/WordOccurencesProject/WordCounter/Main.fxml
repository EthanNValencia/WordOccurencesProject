<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="450.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WordCounter.Controller">
   <children>
      <ImageView fx:id="imageView" fitHeight="242.0" fitWidth="739.0" layoutX="14.0" layoutY="193.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="beginBtn" layoutX="785.0" layoutY="65.0" mnemonicParsing="false" onAction="#downloadPage" text="Download HTML" />
      <TextArea fx:id="textArea" layoutX="14.0" layoutY="145.0" prefHeight="294.0" prefWidth="739.0" />
      <Button fx:id="cleanBtn" layoutX="818.0" layoutY="101.0" mnemonicParsing="false" onAction="#cleanText" text="Clean Text" />
      <Button fx:id="countBtn" layoutX="804.0" layoutY="136.0" mnemonicParsing="false" onAction="#countWords" text="Count Words" />
      <Label layoutX="450.0" layoutY="345.0" />
      <Button fx:id="connect" layoutX="809.0" layoutY="232.0" mnemonicParsing="false" onAction="#getIP" text="Connect" />
      <Button fx:id="noConnect" layoutX="786.0" layoutY="200.0" mnemonicParsing="false" onAction="#doNotConnect" text="Do Not Connect" />
      <Button fx:id="submitBtn" layoutX="776.0" layoutY="415.0" mnemonicParsing="false" onAction="#beginConnection" text="Submit IP Address" />
      <TextField fx:id="ipEntry" layoutX="776.0" layoutY="382.0" prefHeight="25.0" prefWidth="113.0" promptText="Enter IP Address..." />
      <Button fx:id="restartBtn" layoutX="765.0" layoutY="10.0" mnemonicParsing="false" onAction="#initButtons" text="Restart" />
      <Button fx:id="quitBtn" layoutX="825.0" layoutY="10.0" mnemonicParsing="false" onAction="#quitSystem" text="Safe Quit" />
      <Button fx:id="option1" layoutX="819.0" layoutY="58.0" mnemonicParsing="false" onAction="#helpCenter_Option1" prefHeight="39.0" prefWidth="71.0" text="1" />
      <Button fx:id="option2" layoutX="818.0" layoutY="107.0" mnemonicParsing="false" onAction="#helpCenter_Option2" prefHeight="39.0" prefWidth="71.0" text="2" />
      <Button fx:id="option3" layoutX="818.0" layoutY="156.0" mnemonicParsing="false" onAction="#helpCenter_Option3" prefHeight="39.0" prefWidth="71.0" text="3" />
      <Button fx:id="helpCenterBtn" layoutX="810.0" layoutY="263.0" mnemonicParsing="false" onAction="#returnToHelpCenter" text="Help Center" />
      <Button fx:id="opt2Next" layoutX="837.0" layoutY="331.0" mnemonicParsing="false" onAction="#helpCenter_Option2" text="Next &gt;" />
      <Button fx:id="opt2Back" layoutX="838.0" layoutY="297.0" mnemonicParsing="false" onAction="#helpCenter_Option2" text="&lt; Back" />
      <TextArea fx:id="textExplain" layoutX="14.0" layoutY="11.0" prefHeight="119.0" prefWidth="739.0" />
   </children>
</AnchorPane>
